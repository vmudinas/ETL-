      <DTS:Variables />
      <DTS:PropertyExpression
        DTS:Name="[Source - Query].[SqlCommand]">"select TO_DATE(I.INTRADER_COLLTRL_DAILY_DT_ID,'YYYYMMDD') AS INTRADER_COLLTRL_DAILY_DT, 
I.INTRADER_COLLTRL_FR_TICKET_NUM, 
I.COLLTRL_FROM_CUSIP,
D.SECURITY_DESC,
A.COLLTRL_GROUP_DESC, 
A.PLEDGE_CATEGORY_DESC, 
I.COLLTRL_TO_LOCATION, 
1 AS COLLTRL_RECORD_TYPE,  
ROUND((F.ORIG_FACT_AMT * I.COLLTRL_MKT_VALUE) / 
(SELECT SUM(COLLTRL_MKT_VALUE) 
FROM INTRADER_COLLTRL_DAILY_FACT_B  S 
WHERE S.INTRADER_COLLTRL_DAILY_DT_ID = I.INTRADER_COLLTRL_DAILY_DT_ID
AND S.INTRADER_COLLTRL_FR_TICKET_NUM = I.INTRADER_COLLTRL_FR_TICKET_NUM  )) AS ORIG_FACT_AMT,
I.COLLTRL_MKT_VALUE
from INTRADER_COLLTRL_DAILY_FACT_B i
join INTRADER_COLLTRL_ATTRIB_DIM_B a on I.INTRADER_COLLTRL_ATTRIB_DIM_ID = A.INTRADER_COLLTRL_ATTRIB_DIM_ID
join ODMDMQ.INTRADER_DAILY_FACT_B F on I.INTRADER_COLLTRL_FR_TICKET_NUM = F.TICKET_NUM
join ODMDMQ.INTRADER_ACCT_DIM_B d on F.INTRADER_ACCT_DIM_ID = D.INTRADER_ACCT_DIM_ID
where INTRADER_COLLTRL_DAILY_DT_ID = " + @[User::as_of_date_yyyymmdd] + "
AND F.INTRADER_DAILY_DATE_ID = " + @[User::as_of_date_yyyymmdd] + "
AND  I.COLLTRL_RECORD_TYPE = 'PLEDGE'
and TO_DATE(" + @[User::as_of_date_yyyymmdd] + ",'YYYYMMDD') between D.begin_dt and D.end_dt
UNION ALL
select TO_DATE(F.INTRADER_DAILY_DATE_ID,'YYYYMMDD') AS INTRADER_COLLTRL_DAILY_DT, 
F.TICKET_NUM AS INTRADER_COLLTRL_FR_TICKET_NUM,
F.CUSIP AS COLLTRL_FROM_CUSIP,
A.SECURITY_DESC,
A.INTRADER_GROUP_DESC AS COLLTRL_GROUP_DESC,
'-' AS PLEDGE_CATEGORY_DESC,
'-' AS COLLTRL_TO_LOCATION,
0 AS COLLTRL_RECORD_TYPE,
F.ORIG_FACT_AMT,
F.FAIR_VALUE AS COLLTRL_MKT_VALUE
from ODMDMQ.INTRADER_DAILY_FACT_B F
join ODMDMQ.INTRADER_ACCT_DIM_B A on F.INTRADER_ACCT_DIM_ID = A.INTRADER_ACCT_DIM_ID
where F.INTRADER_DAILY_DATE_ID = " + @[User::as_of_date_yyyymmdd] + "
and TO_DATE(" + @[User::as_of_date_yyyymmdd] + ",'YYYYMMDD') between A.begin_dt and A.end_dt
and A.INTRADER_GROUP_DESC != '-' and F.PORT_ID NOT IN ('CM1/TRES','AAIGMUNI','AMUNIPHA')" </DTS:PropertyExpression>
