ODMCUST.DDA_ACCOUNT_T1_DIM_B.DDA_ACCOUNT_NUM AS AccountNumber,
ODMCUST.DDA_ACCOUNT_T1_DIM_B.DDA_ACCOUNT_TAX_ID_NUM AS TaxId,
ODMCUST.DDA_ACCOUNT_T1_DIM_B.DDA_BRANCH_STATE_CD AS StateCode,
ODMCUST.DDA_ACCOUNT_T1_DIM_B.DDA_INT_BEARING_CD AS IntBearingCode,
ROUND(ODMCUST.DDA_DAILY_BALANCE_FACT_B.DDA_COLLECTED_BALANCE_AMT,0) AS CollectedBalanceAmount,
ROUND(ODMCUST.DDA_DAILY_BALANCE_FACT_B.DDA_DAILY_ACCRUED_INT_AMT,0) AS AccruedInterest
FROM ODMCUST.DDA_DAILY_BALANCE_FACT_B,OSSTP.DATE_DIM_B,ODMCUST.DDA_ACCOUNT_T1_DIM_B
WHERE ( OSSTP.DATE_DIM_B.DATE_ID=ODMCUST.DDA_DAILY_BALANCE_FACT_B.DDA_DAILY_BALANCE_DATE_ID  )
AND  ( ODMCUST.DDA_ACCOUNT_T1_DIM_B.DDA_ACCOUNT_NUM=ODMCUST.DDA_DAILY_BALANCE_FACT_B.DDA_ACCOUNT_NUM  )
AND ( OSSTP.DATE_DIM_B.CALENDAR_DT  =  to_Date( 20140419, 'yyyymmdd' )
AND ODMCUST.DDA_ACCOUNT_T1_DIM_B.DDA_ACCOUNT_TYPE_CD  =  '4' )
AND (ODMCUST.DDA_DAILY_BALANCE_FACT_B.DDA_COLLECTED_BALANCE_AMT &gt; 0 
OR (ODMCUST.DDA_DAILY_BALANCE_FACT_B.DDA_DAILY_ACCRUED_INT_AMT &gt; 0
