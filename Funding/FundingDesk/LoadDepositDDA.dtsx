                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT OSSTP.DATE_DIM_B.CALENDAR_DT AS AS_OF_DATE, 
ODMCUST.DDA_ACCOUNT_T1_DIM_B.DDA_ACCOUNT_TYPE_CD AS AccountTypeCode,
ODMCUST.DDA_ACCOUNT_T1_DIM_B.DDA_PUBLIC_FUND_LK_TYPE_CD AS LkUpTypeCode,
ODMCUST.DDA_ACCOUNT_T1_DIM_B.DDA_PUBLIC_FUND_LK_TYPE_DESC AS LkUpTypeDesc,
