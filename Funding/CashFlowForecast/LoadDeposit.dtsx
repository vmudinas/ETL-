      <DTS:PropertyExpression
        DTS:Name="[Source - Query].[SqlCommand]">"SELECT
    DATE_DIM.CALENDAR_DT,
    ORG.ORG_HIER_LVL3_ID_CURR,
   ORG.ORG_HIER_LVL3_DESC_CURR,
   (sum(OEDWDDS.PS_GL_DAILY_LEDGER_FACT_B.GL_ENDING_BAL_AMT) * -1) GL_CUR_BAL_AMT
  FROM
  OEDWDDS.PS_GL_DAILY_LEDGER_FACT_B,
  OEDWDDS.ORGANIZATION_DIM_B  ORG,
  OSSTP.DATE_DIM_B  DATE_DIM,
  OEDWDDS.GL_ACCOUNT_DIM_B  ACCT
WHERE
  ( OEDWDDS.PS_GL_DAILY_LEDGER_FACT_B.PS_GL_LEDGER_DT_ID=DATE_DIM.DATE_ID  )
  AND  ( OEDWDDS.PS_GL_DAILY_LEDGER_FACT_B.GL_ACCT_DIM_ID=ACCT.GL_ACCT_ID  )
  AND  ( OEDWDDS.PS_GL_DAILY_LEDGER_FACT_B.ORG_DIM_ID=ORG.ORG_ID  )
  AND DATE_DIM.CALENDAR_DT = TO_DATE('" + (DT_WSTR,20) @[User::as_of_date] + "','MM/DD/YYYY')
  AND  
  (
   ACCT.PS_GL_ACCT_LVL02_ID  =  '100309'
 )
GROUP BY
    DATE_DIM.CALENDAR_DT,
    ORG.ORG_HIER_LVL3_ID_CURR,
   ORG.ORG_HIER_LVL3_DESC_CURR
   union
SELECT
    PREV.CALENDAR_DT,
    ORG.ORG_HIER_LVL3_ID_CURR,
   ORG.ORG_HIER_LVL3_DESC_CURR,
   (sum(OEDWDDS.PS_GL_DAILY_LEDGER_FACT_B.GL_ENDING_BAL_AMT) * -1) GL_CUR_BAL_AMT
  FROM
  OEDWDDS.PS_GL_DAILY_LEDGER_FACT_B,
  OEDWDDS.ORGANIZATION_DIM_B  ORG,
  OSSTP.DATE_DIM_B  DATE_DIM,
  OSSTP.DATE_DIM_B  PREV,    
  OEDWDDS.GL_ACCOUNT_DIM_B  ACCT
WHERE
  ( OEDWDDS.PS_GL_DAILY_LEDGER_FACT_B.PS_GL_LEDGER_DT_ID=PREV.DATE_ID  )
  AND  ( OEDWDDS.PS_GL_DAILY_LEDGER_FACT_B.GL_ACCT_DIM_ID=ACCT.GL_ACCT_ID  )
  AND  ( OEDWDDS.PS_GL_DAILY_LEDGER_FACT_B.ORG_DIM_ID=ORG.ORG_ID  )
  AND PREV.CALENDAR_DT = DATE_DIM.PRIOR_BUSINESS_CALENDER_DT  
  AND DATE_DIM.CALENDAR_DT = TO_DATE('" + (DT_WSTR,20) @[User::as_of_date] + "','MM/DD/YYYY')
  AND  
  (
   ACCT.PS_GL_ACCT_LVL02_ID  =  '100309'
  )
GROUP BY
    PREV.CALENDAR_DT,
    ORG.ORG_HIER_LVL3_ID_CURR,
   ORG.ORG_HIER_LVL3_DESC_CURR"</DTS:PropertyExpression>
