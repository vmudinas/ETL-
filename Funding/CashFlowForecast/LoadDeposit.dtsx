  AND  ( OEDWDDS.GL_DAILY_BALANCE_FACT_B.GL_ACCT_ID=ACCT.GL_ACCT_ID  )
  AND  ( OEDWDDS.GL_DAILY_BALANCE_FACT_B.ORG_ID=ORG.ORG_ID  )
  AND DATE_DIM.CALENDAR_DT = TO_DATE('9/6/2013','MM/DD/YYYY')
  AND  
  (
   ACCT.GL_ACCT_LVL3_NUM  =  210000
   AND
   ORG.ORG_HIER_LVL4_DESC_CURR  IN  ( 'COMMERCIAL TREASURY SERVICES','CORPORATE &amp; INSTITUTIONAL TREAS SVCS','GLOBAL TREASURY SERVICES','GLOBAL TRUST &amp; TREASURY SOLUTIONS'  )
  )
GROUP BY
    DATE_DIM.CALENDAR_DT,
    ORG.ORG_HIER_LVL4_ID_CURR,
   ORG.ORG_HIER_LVL4_DESC_CURR
   union
SELECT
