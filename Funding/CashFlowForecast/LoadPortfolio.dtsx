      <DTS:Variables />
      <DTS:PropertyExpression
        DTS:Name="[Source - Query].[SqlCommand]">"SELECT TO_DATE(TO_CHAR(FACT.INTRADER_DAILY_DATE_ID),'YYYYMMDD') AS BUSINESS_DATE,
            FACT.PORT_ID, 
            ACCT.INTRADER_ACCT_SETTLE_DT AS PAYMENT_DATE, 
            'S' AS PAYMENT_TYPE,
            SUM(ORIG_PRIN_AMT) AS BALANCE
FROM ODMDMQ.INTRADER_DAILY_FACT_B FACT, 
          ODMDMQ.INTRADER_ACCT_DIM_B ACCT
WHERE FACT.INTRADER_ACCT_DIM_ID = ACCT.INTRADER_ACCT_DIM_ID 
AND FACT.INTRADER_DAILY_DATE_ID = '" + @[User::as_of_date_yyyymmdd] + "'
AND ACCT.INTRADER_ACCT_SETTLE_DT &gt;= TO_DATE('" +@[User::as_of_date_yyyymmdd] +"','YYYYMMDD')
AND FACT.PORT_ID in ( "+ @[User::port_codes] +" ) 
GROUP BY FACT.INTRADER_DAILY_DATE_ID, FACT.PORT_ID, ACCT.INTRADER_ACCT_SETTLE_DT
UNION
SELECT TO_DATE(TO_CHAR(FACT.INTRADER_DAILY_DATE_ID),'YYYYMMDD') AS BUSINESS_DATE,
            FACT.PORT_ID, 
            ACCT.INTRADER_ACCT_MTRTY_DT AS PAYMENT_DATE, 
            'M' AS PAYMENT_TYPE,
            SUM(ORIG_PAR_AMT + (CASE WHEN ORIG_PAR_AMT &gt; 0 THEN ACCRUED_INT_AMT ELSE ACCRUED_INT_AMT * -1 END )) AS BALANCE
FROM ODMDMQ.INTRADER_DAILY_FACT_B FACT, 
          ODMDMQ.INTRADER_ACCT_DIM_B ACCT
WHERE FACT.INTRADER_ACCT_DIM_ID = ACCT.INTRADER_ACCT_DIM_ID 
AND FACT.INTRADER_DAILY_DATE_ID = '" + @[User::as_of_date_yyyymmdd] + "'
AND ACCT.INTRADER_ACCT_MTRTY_DT &gt;= TO_DATE('" +@[User::as_of_date_yyyymmdd] +"','YYYYMMDD')
AND FACT.PORT_ID in ( " + @[User::port_codes] + " )
GROUP BY FACT.INTRADER_DAILY_DATE_ID, FACT.PORT_ID, ACCT.INTRADER_ACCT_MTRTY_DT
UNION
SELECT TO_DATE( " + @[User::as_of_date_yyyymmdd] + ",'YYYYMMDD') AS BUSINESS_DATE,
            FACT.PORT_ID, 
            ACCT.INTRADER_ACCT_MTRTY_DT AS PAYMENT_DATE, 
            'M' AS PAYMENT_TYPE,
            SUM(ORIG_PAR_AMT + (CASE WHEN ORIG_PAR_AMT &gt; 0 THEN ACCRUED_INT_AMT ELSE ACCRUED_INT_AMT * -1 END )) AS BALANCE
FROM ODMDMQ.INTRADER_DAILY_FACT_B FACT, 
          ODMDMQ.INTRADER_ACCT_DIM_B ACCT
WHERE FACT.INTRADER_ACCT_DIM_ID = ACCT.INTRADER_ACCT_DIM_ID 
AND FACT.INTRADER_DAILY_DATE_ID = '" + @[User::prev_as_of_date_yyyymmdd] + "'
AND ACCT.INTRADER_ACCT_MTRTY_DT = TO_DATE('" +@[User::as_of_date_yyyymmdd] +"','YYYYMMDD')
AND FACT.PORT_ID in ( " + @[User::port_codes] + " )
GROUP BY FACT.INTRADER_DAILY_DATE_ID, FACT.PORT_ID, ACCT.INTRADER_ACCT_MTRTY_DT"</DTS:PropertyExpression>
